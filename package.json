{
  "name": "dome",
  "version": "0.1.0",
  "description": "Aplicación de escritorio inteligente para gestión de conocimiento e investigación",
  "author": "Dome Team",
  "main": "electron/main.cjs",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"bun run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "bun run build && bun run rebuild:natives && bun run verify:natives && DEBUG= electron-builder",
    "electron:build:verbose": "bun run build && bun run rebuild:natives && bun run verify:natives && DEBUG=electron-builder electron-builder",
    "rebuild:natives": "electron-rebuild -f -w=better-sqlite3,sharp,@ffmpeg-installer/ffmpeg",
    "verify:natives": "node scripts/verify-natives.cjs",
    "copy:pdf-worker": "node scripts/copy-pdf-worker.cjs",
    "postinstall": "electron-builder install-app-deps && bun run rebuild:natives && bun run copy:pdf-worker",
    "test:db": "bun run scripts/test-db.ts",
    "clean": "bash scripts/clean.sh",
    "generate-icons": "bun run scripts/generate-icons.ts"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@sinclair/typebox": "^0.34.48",
    "@tiptap/extension-bubble-menu": "^3.17.1",
    "@tiptap/extension-code-block-lowlight": "^2.6.0",
    "@tiptap/extension-color": "^3.17.1",
    "@tiptap/extension-dropcursor": "^3.17.1",
    "@tiptap/extension-floating-menu": "^3.17.1",
    "@tiptap/extension-gapcursor": "^3.17.1",
    "@tiptap/extension-highlight": "^2.6.0",
    "@tiptap/extension-image": "^2.6.0",
    "@tiptap/extension-link": "^2.6.0",
    "@tiptap/extension-placeholder": "^2.6.0",
    "@tiptap/extension-subscript": "^3.17.1",
    "@tiptap/extension-superscript": "^3.17.1",
    "@tiptap/extension-table": "^3.17.1",
    "@tiptap/extension-table-cell": "^3.17.1",
    "@tiptap/extension-table-header": "^3.17.1",
    "@tiptap/extension-table-row": "^3.17.1",
    "@tiptap/extension-task-item": "^2.6.0",
    "@tiptap/extension-task-list": "^2.6.0",
    "@tiptap/extension-text-align": "^3.17.1",
    "@tiptap/extension-text-style": "^3.17.1",
    "@tiptap/extension-typography": "^3.17.1",
    "@tiptap/extension-underline": "^3.17.1",
    "@tiptap/react": "^2.6.0",
    "@tiptap/starter-kit": "^2.6.0",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "apache-arrow": "^14.0.2",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dagre": "^0.8.5",
    "electron-ollama": "^0.1.25",
    "fluent-ffmpeg": "^2.1.3",
    "lowlight": "^3.1.0",
    "lucide-react": "^0.460.0",
    "mammoth": "^1.11.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.624",
    "qrcode-terminal": "^0.12.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.5.0",
    "tar": "^7.5.7",
    "tippy.js": "^6.3.7",
    "vectordb": "^0.4.0",
    "xlsx": "^0.18.5",
    "yauzl": "^3.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.3",
    "@types/better-sqlite3": "^7.6.0",
    "@types/bun": "latest",
    "@types/dagre": "^0.7.53",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.0.0",
    "electron": "^32.0.0",
    "electron-builder": "^25.0.0",
    "electron-devtools-installer": "^4.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vite": "^7.3.1",
    "wait-on": "^8.0.0"
  },
  "build": {
    "appId": "com.domepro.app",
    "productName": "Dome",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "prompts/**/*",
      "package.json",
      "!**/node_modules/*/{CHANGELOG.md,README.md,readme.md,readme,test,__tests__,tests,powered-test,*.ts,*.map}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/node_modules/*/{example,examples,doc,docs}"
    ],
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "node_modules/sharp/**/*",
      "node_modules/@img/**/*",
      "node_modules/vectordb/**/*",
      "node_modules/@lancedb/**/*",
      "node_modules/apache-arrow/**/*"
    ],
    "afterPack": "./scripts/after-pack.cjs",
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "notarize": true
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon-256.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}